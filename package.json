  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:server": "webpack --config webpack.dev.server.js",
    "build:client": "cp -rp ./src/public ./dist/public && webpack --config webpack.dev.client.js",
    "start": "node dist/server.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "jest .",
    "coverage": "jest --coverage && codeclimate-test-reporter < coverage/lcov.info",
    "eslint": "eslint ."
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "/spec/.*\\_spec.js$",
    "automock": true,
    "cacheDirectory": "/tmp/foogle",
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-css-transition-group",
      "react-dom",
      "react-router",
      "redux",
      "redux-thunk",
      "redux-logger",
      "enzyme",
